# === Supabase PostgreSQL 連線設定 ===
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=db.xxxxxxx.supabase.co
DB_POSTGRESDB_PORT=6543
DB_POSTGRESDB_USER=postgres.xxxxxxx
DB_POSTGRESDB_PASSWORD=your-supabase-password
DB_POSTGRESDB_DATABASE=postgres

# === n8n 設定 ===
N8N_ENCRYPTION_KEY=your-encryption-key-here
N8N_PROTOCOL=https
N8N_PORT=7860

# === 網址設定（替換 <username> 為你的 HuggingFace 使用者名稱）===
WEBHOOK_URL=https://<username>-n8n.hf.space/
N8N_EDITOR_BASE_URL=https://<username>-n8n.hf.space/

# === 時區 ===
GENERIC_TIMEZONE=Asia/Taipei
TZ=Asia/Taipei
